    "@prisma/client": "^5.10.2",
    "@tiptap/pm": "2.1.13",
    "@tiptap/react": "2.1.13",
    "@tiptap/starter-kit": "2.1.13",
    "adminjs": "^7.8.17",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.68.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-formidable": "^1.2.0",
    "express-openapi-validator": "^5.6.2",
    "express-session": "^1.19.0",
    "helmet": "^7.1.0",
    "http-status": "^2.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "sanitize-html": "^2.17.0",
